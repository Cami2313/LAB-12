---
import NavbarPrimary from '../components/NavbarPrimary.astro';
import NavbarSecondary from '../components/NavbarSecondary.astro';
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<NavbarPrimary />
		<hr>
		<NavbarSecondary />
	</body>
</html>

